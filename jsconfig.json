{
    "compilerOptions": {
        "baseUrl": ".",
        "noEmit": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowJs": true,
        "checkJs": true,
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noPropertyAccessFromIndexSignature": true,
    },
    "exclude": [
        "node_modules"
    ]
}